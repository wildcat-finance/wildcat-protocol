{
  "plugins": [
    "prettier"
  ],
  "rules": {
    "compiler-version": [
      "error",
      ">=0.8.20"
    ],
    "func-visibility": [
      "warn",
      {
        "ignoreConstructors": true
      }
    ],
    "no-empty-blocks": "off",
    "no-inline-assembly": "off",
    "avoid-low-level-calls": "off",
    "not-rely-on-time": "off",
    "var-name-mixedcase": "off",
    "func-name-mixedcase": "off",
    "max-line-length": [
      "warn",
      100
    ],
    "function-max-lines": "off",
    "code-complexity": [
      "warn",
      15
    ],
    "private-vars-leading-underscore": "off"
  }
}